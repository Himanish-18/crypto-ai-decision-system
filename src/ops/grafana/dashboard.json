{
    "title": "Crypto HFT Dashboard",
    "panels": [
        {
            "title": "Latency (p99)",
            "type": "graph",
            "targets": [
                {
                    "expr": "histogram_quantile(0.99, rate(trade_latency_seconds_bucket[5m]))"
                }
            ]
        },
        {
            "title": "Slippage %",
            "type": "gauge",
            "targets": [
                {
                    "expr": "trade_slippage_pct"
                }
            ]
        }
    ]
}